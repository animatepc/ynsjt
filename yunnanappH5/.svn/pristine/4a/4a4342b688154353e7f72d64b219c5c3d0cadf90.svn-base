.menu{
    position: fixed;
    left: 0;
    bottom: 0;
    z-index: 1;
    background-color: #FFF;
    .memu-list{
        height: .68rem;
        display:flex;
        border-top: .01rem solid #cacaca;
        padding: .15rem .20rem;
        >li{
            width: 20%;
            font-size: .12rem;
            .menu_icon{
                margin: 0 auto;
                width: 0.44rem;
                height: 0.38rem;
                background-repeat: no-repeat;
                background-size: contain;
            }
            .menu_active{
                color: #f51c11;
            }
        }
    }
}