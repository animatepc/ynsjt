//自定义酒店详情
.cusDetail{
  .title{
    box-shadow: 0 1px 0 0 @shadowColor;
    background: @contentBG;
    border-bottom: 1px solid @shadowColor;
  }
  .hotelCon{
    box-shadow: 0 1px 0 0 @shadowColor;
    background: @contentBG;
    border-right: 1px solid @shadowColor;
    padding: 25px 40px 30px;
    position: relative;
  }
  .contactInfo{
    background: @contentBG;
    box-shadow: 0 1px 0 0 @shadowColor;
    padding: 25px 80px 30px;
    position: relative;
  }
  .twoGraTitle{
    color: @defaultFont
  }
  .itemKey{
    color: @defaultFont;
  }
  .hotSatImg1{
    width: 48px;
    height: 22px;
//  background: url("../../static/imgs/group1.png") center center no-repeat;
    display: inline-block;
  }
  .hotSatIcon1{
    width: 125px;
    height: 108px;
//  background: url("../../static/imgs/group1_1.png") center center no-repeat;
    display: inline-block;
    position: absolute;
    right: 5%;
    top: 25px;
  }
  .BMap{

  }
  .activeType{
    color: @mainBlue !important;
  }
  .curTypeStyle{
    color: @defaultFont !important;
  }
  .curTypeStyle:hover{
    color: @mainBlue !important; ;
  }
  .picGroup{
    box-shadow: 0 1px 0 0 @shadowColor;
    padding: 40px 40px 30px 40px;
    background:@contentBG ;
    overflow: hidden;
  }
  //自定义select
  .selectGroup{
    padding:1px;
    background: @mainBG;
    width: 100%;
    z-index: 3;
  }
  .selectItem{
    height:28px ;
    line-height: 28px;
    cursor: pointer;
    padding-left: 5px;
    padding-right: 5px;
  }
  .selectItem:hover{
    background: @mainBlue;
    color: @contentBG;
  }
  .selIcon{
    display: inline-block;
    position: absolute;
    right: 0;
    width:10px ;
    height: 5px;
    top:15px;
    // background: url("../../static/imgs/ic_xiala.png");
  }

  //轮播部分
  @media only screen and (max-width: 2000px) {
    .addPic{
      background-size: contain;
    }

  }
  @media only screen and (min-width: 2001px) {
    .addPic{
      background-size: cover;
    }
  }
  .leftBtn{
    cursor: pointer;
    //width: 2%;
    display: inline-block;
    // background: url("../../static/imgs/leftbtnactive.png") center center no-repeat;
    //margin-right: 1.5%;
    float: left;
    height: 100%;

  }
  .leftBtnDisable{
    //width: 2%;
    display: inline-block;
    // background: url("../../static/imgs/leftbtn.png") center center no-repeat;
    //margin-right: 1.5%;
    float: left;
    height: 100%;

  }
  .rightBtn{
    cursor: pointer;
    //width: 2%;
    display: inline-block;
    // background: url("../../static/imgs/rightbtnactive.png") center center no-repeat;
    //margin-left: 1.5%;
    float: left;
    height: 100%;
  }
  .rightBtnDisable{
    //width: 2%;
    display: inline-block;
    // background: url("../../static/imgs/rightbtn.png") center center no-repeat;
    //margin-left: 1.5%;
    float: left;
    height: 100%;
  }
  .carContent{
    //height: 173px;
    float: left;
  }
  .carImg{
    display: inline-block;
    //height: 173px;
    //width: 93%;
    overflow: hidden;
    float: left;
  }
  .addPic{
    position: relative;
    z-index: 22;
    cursor: pointer;
    //width: 20%;
    height: 100%;
    display: inline-block;
    // background: url("../../static/imgs/addImg.png") center center no-repeat;
    background-size: contain;
    user-select: unset;
  }
  .picContent{
    //width: 80%;
    height: 100%;
    position: relative;
    z-index: 1;
  }
  .picContentT{
    width:4000px ;
    float: left;
    height: 100%;
  }
  .picList{
    float: left;
    //width: 24px;
    height: 100%;
    //margin-left: 1%;
    display: inline-block;
    //background: url("../../static/imgs/nopic.png") center center no-repeat;
    background-size: cover !important;

  }
  .maskLayer{
    width: 100%;
    height: 100%;
    background:@contentBlack ;
    display: inline-block;
    opacity: .7;
    position: relative;

  }
  .coverGroup{
    position: absolute;
    left: 0;
    color: @contentBG;
    height: 100%;
    width: 50%;
    text-align: center;
    top: 50%;
    transform: translateY(-23%);
  }
  .coverIcon{
    cursor: pointer;
    display: block;
    width: 30%;
    height: 30%;
    // background: url("../../static/imgs/coverIcon.png") center center no-repeat;
    background-size: contain;
    margin: auto;
  }
  .deleteGroup{
    position: absolute;
    right: 0;
    color: @contentBG;
    height: 100%;
    width: 50%;
    text-align: center;
    top: 50%;
    transform: translateY(-23%);
  }
  .deleteIcon{
    cursor: pointer;
    margin: auto;
    display: block;
    width: 30%;
    height: 30%;
    // background: url("../../static/imgs/deleteIcon.png") center center no-repeat;
    background-size: contain;
  }
  .maskText{
    cursor: pointer;
  }
  .alreadyCover{
    width: 100%;
    height: 100%;
    position: relative;
    display: inline-block;
  }
  .alrCoverIcon{
    position: absolute;
    display: inline-block;
    width: 30%;
    height: 44%;
    right: 0;
    top: 1px;
    // background: url("../../static/imgs/alreadycover.png") center center no-repeat;
    background-size: contain;
  }

}
//自定义table
.cusTable{
  .tableHeader{
    display: flex;
    justify-content: space-between;
    background: @mainBorder;
  }
  .tableBody{
    .bodyItem{
      box-shadow: 0 1px 0 0 @shadowColor;
      border-bottom: 1px solid transparent;
      border-top: 1px solid transparent;
    }
    .bodyItem:hover{
      background: @TwoGradeBlue;
      border-top:1px solid @mainBlue ;
      border-bottom:1px solid @mainBlue;
    }
    background: @contentBG;
  }
}

//刷新
.refreshIcon{
  cursor: pointer;
  width:36px ;
  height: 36px;
  // background:url("../../static/imgs/shuaxin_nor.png") center center;
  background-size: cover;
}
.refreshIcon:hover{
  cursor: pointer;
  width:36px ;
  height: 36px;
  // background:url("../../static/imgs/shuaxin.png") center center;
  background-size: cover;
}
.indexGroup{
  position: absolute;
  top: 17px;
  right: 30px;
  height: 70px;
}
 //element dialog
.el-dialog{
  .el-dialog__title{
    font-size: 14px;
    color:@contentBlack;
  }
  .el-dialog__body{
    font-size: 12px;
    padding: 20px;
    color:@TwoGradeTitle;
  }
  .el-button--primary{
    font-size: 14px;
    border-radius: 2px;


  }
   .el-button--default{
     color: @contentBlack;
     border:0 none;
  }

}
.noHeader{
  .el-dialog__header{
    display: none;
  }
}
.headerLeft{
  .el-dialog__header{
    text-align: left;
  }
}
.footerRight{
  .el-dialog__footer{
    text-align: right;
  }
}

//element button
.el-button{
  padding: 10px 20px;
  border-radius: 2px;
  font-weight: 400;
}
.el-input__inner{
  height: 36px;
  border: 1px solid @borderccc;
  border-radius: 2px;
  font-size: 12px;
}
.el-input.is-disabled{
  background: @mainBG;
}
.input__inner:focus{
  border-color: @mainBlue;
}
.el-input__innerErro{
  height: 36px;
  border: 1px solid red !important;
  border-radius: 2px;
  font-size: 12px;
}
.el-button--success{
  color:@contentBG;
  background-color: @mainGreen;
  border-color: @mainGreen;
}
.el-button--primary{
  color:@contentBG;
  background-color:@mainBlue;
  border-color: @mainBlue;
}
.hasBorder{
  border: 1px solid @borderccc !important;
  border-radius: 2px !important;
}
//el-notification
.el-notification{
  border: 1px solid @mainBorder;
  box-shadow: 0 1px 12px 0 rgba(0,0,0,.1);
  .el-notification__title{
    font-size: 14px;
    color:@menuFont ;
  }
  .el-notification__content{
    font-size: 12px;
    color:@defaultFont ;
  }
}
.el-dialog__wrapper{
  z-index: 3110 !important;
}
.v-modal{
  z-index: 3100 !important;
}
.el-notification {
  z-index: 4000 !important;
}
