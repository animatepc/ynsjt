.login_alert{
  .weui-dialog{
      border-radius: .12rem;
      max-width: 80%; 
  }
   @media screen and (min-width: 1024px){
    .weui-dialog {
        width: 70% !important;
    }
}
  .weui-dialog__bd, .weui-dialog__hd, .weui-dialog__ft{
      font-size: .32rem;
  }
  .dialog__hd{
      border:none;
  }
  .weui-dialog__ft{
      width: 1.8rem;
      height: 1.2rem;
      line-height: .8rem;
      margin: 0 auto;
      padding-bottom: .4rem;
      >a{
          display: block;
          color: #fff;
          background-color: #f41a14;
          border: none;
          box-shadow: none;
          font-weight: 500;
          -webkit-border-radius: 4px;
          border-radius: .12rem;
          margin: 0;
          cursor: pointer;
      }
  }
}
.confirm_alert {
  .weui-dialog {
    max-width: 80%;
    border-radius: 0.12rem;
    // border:none;
  }
  .weui-dialog__bd,
  .weui-dialog__hd,
  .weui-dialog__ft {
    font-size: 0.32rem;
  }
  .dialog__hd {
    border: none;
  }
  .weui-dialog__ft {
    width: 100%;
    padding: 0 0.4rem;
    line-height: 0.8rem;
    margin: 0 auto;
    padding-bottom: 0.4rem;
    .weui-dialog__btn_default {
      background-color: #c1c1c1;
      margin-rigfht: 0.4rem;
      margin-left: 0;
    }
    > a {
      margin-left: 0.4rem;
      background-color: #f41a14;
      color: #fff;
      font-weight: 500;
      border-radius: 0.12rem;
    }
  }
  @media screen and (min-width: 1024px){
    .weui-dialog {
        width: 80%;
    }
  }
}
