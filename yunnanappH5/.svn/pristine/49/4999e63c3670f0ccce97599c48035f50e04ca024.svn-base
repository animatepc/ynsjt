.header{
    width: 7.1rem;
    height: 1rem;
    padding: .14rem .2rem;
    padding-top: 0.4rem;
    font-size: .36rem;
    position: fixed;
    top: 0;
    left: 0;
    overflow: hidden;
    background-color: #f41a14;
    z-index: 2;
    .search_photo{
        width: 5.35rem;
    }
    .gobroke{
        right: .3rem;
        font-size: 0.4rem;
        color: #fff;
    }
    .search_box{
        height:.5rem;
        border: .02rem solid #fff;
        border-radius: .1rem;
        .search_icon{
            width: .3rem;
            height: .3rem;
            background: url('../../static/imgs/search.png') center center no-repeat;
            background-size: .3rem .3rem;
            z-index: 1;
            top: 0;
            left: 2rem;
            bottom: 0;
            margin: auto;
        }
        .search_mode{
            width: 2.85rem;
            height: 100%;
            left: 2.4rem;
            overflow: hidden;
            outline: none;
            border: 0rem solid #fff;
            background-color: #f41a14;
            color: #fff;
            &::-webkit-input-placeholder{ /*WebKit browsers*/
                color: #fff;
            }
            &::-moz-input-placeholder{ /*Mozilla Firefox*/
                color: #fff;
            }
            &::-ms-input-placeholder{ /*Internet Explorer*/ 
                color: #fff;
            }
        }
    }
    >.search_city{
        text-align: right;
        width: 1.8rem;
        height: .5rem;
        line-height: .5rem;
        font-size: .24rem;
        position: relative;
        .menu_location{
            width: .3rem;
            height: .3rem;
            background: url('../../static/imgs/gps_icon.png') center center no-repeat;
            background-size: .3rem .3rem;
            position: absolute;
            top: 50%;
            bottom: 50%;
            left: 40%;
            right: 60%;
            transform: translate(-50%,-50%); 
        }
    }
    .search{
        height: 100vh;
        background-color: #fff;
        margin-top: 1.52rem;
        .searchVal{
            width: 7.5rem;
            height: 1rem;
            padding: .14rem 0;
            padding-top: 0.4rem;
            position: fixed;
            top: 0;
            left: 0;
            overflow: hidden;
            background-color: #f41a14;
            .search_back{
                width: 0.22rem;
                height: 0.4rem;
                padding: 0 0.3rem;
                display: inline-block;
                background: url('../../static/imgs/back.png') center center no-repeat;
                z-index: 10;
            }
            .search_box{
                height:.5rem;
                color: #fff;
                flex: 1;
                position: relative;
                overflow: hidden;
                border: .02rem solid #fff;
                border-radius: .05rem;
                .search_icon{
                    width: .3rem;
                    height: .3rem;
                    background: url('../../static/imgs/search.png') center center no-repeat;
                    background-size: .3rem .3rem;
                    z-index: 1;
                    top: 0;
                    left: 2rem;
                    bottom: 0;
                    margin: auto;
                }
                .search_mode{
                    width: 2.80rem;
                    height: 100%;
                    left: 2.4rem;
                    overflow: hidden;
                    outline: none;
                    border: 0rem solid #fff;
                    background-color: #f41a14;
                    color: #fff;
                    &::-webkit-input-placeholder{ /*WebKit browsers*/
                        color: #fff;
                    }
                    &::-moz-input-placeholder{ /*Mozilla Firefox*/
                        color: #fff;
                    }
                    &::-ms-input-placeholder{ /*Internet Explorer*/ 
                        color: #fff;
                    }
                }
            }
            .search_city{
                height: .5rem;
                line-height: .5rem;
                margin: 0 0.3rem;
                color: #fff;
                overflow: hidden;
                font-size: .24rem;
                position: relative;
            }
            .city_text{
                position: absolute;
                width: 100%;
                font-size: .32rem;
                text-align: center;
            }
        }
        .search_content{
            margin-top: 1.28rem;
            .city_list{
                >li{
                        height: 0.9rem;
                        text-align: center;
                        line-height: 0.9rem;
                        border-bottom: 1px solid #eeeeee;
                }
            }
        }
    }    
}