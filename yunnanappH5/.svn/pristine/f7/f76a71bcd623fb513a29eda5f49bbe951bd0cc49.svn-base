#play * {
  box-sizing: border-box;
}

.video-lockBox {
  position: absolute;
  display: none;
  width: 4em;
  height: 4em;
  left: 2em;
  top: 0;
  bottom: 0;
  margin: auto;
  z-index: 204;
}

.video-lockShadow {
  z-index: 203;
  position: absolute;
  display: none;
  left: 0;
  right: 0;
  bottom: 0;
  top: 0;
}

.video-lock .video-lockShadow {
  display: block;
}

.video-lockBox:after {
  content: ' ';
  background: url(../images/u536.png);
  background-size: contain;
  background-repeat: no-repeat;
  height: .45rem;
  display: block;
}

.video-lock .video-lockBox:after {
  content: ' ';
  background: url(../images/u538.png);
  background-size: contain;
  background-repeat: no-repeat;
  height: .45rem;
  display: block;
}

.video-dialog, .video-range-dialog {
  background: rgba(0, 0, 0, .6);
  position: absolute;
  height: .8rem;
  width: 2.5rem;
  top: .5rem;
  left: 0;
  right: 0;
  color: #fff;
  border-radius: .3rem;
  padding: .1rem .3rem;
  margin: auto;
}

.togglePlayerBox {
  background: rgba(0, 0, 0, .6);
  position: absolute;
  z-index: 200;
  height: .8rem;
  width: .8rem;
  margin: auto;
  top: 0;
  left: 0;
  right: 0;
  bottom: 0;
}

.vjs-paused .togglePlayerImage {
  background: url('../images/u476.png');
  background-size: 50%;
  width: 100%;
  height: 100%;
  background-repeat: no-repeat;
  background-position: center center;
}

.vjs-playing .togglePlayerImage {
  background: url('../images/u486.png');
  background-size: 50%;
  width: 100%;
  height: 100%;
  background-repeat: no-repeat;
  background-position: center center;
}

.rangeDialogText {
  margin-bottom: .25rem;
  text-align: center;
  font-size: .3rem
}

.rangeDialogInput {
  height: .03rem;
}

.rangeDialogContent {
  height: 100%;
  background: #08b3bb;
}

.video-js .togglePlayerBox {
  display: none;
}

.vjs-controls-disabled .video-tools {
  display: block;
}

.video-js .video-tools {
  display: none;
}

.vjs-has-started .video-tools, .vjs-has-started .togglePlayerBox, .full .video-lockBox, .full .video-brightness, .full .video-volume {
  display: block;
}

.full .video-lockBox {
  opacity: .7;
}

.vjs-has-started.vjs-user-inactive.vjs-playing .video-tools, .vjs-has-started.vjs-user-inactive.vjs-playing .togglePlayerBox {
  visibility: visible;
  display: none;
  transition: visibility 1s, opacity 1s;
}

